generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model users {
  id       String @id @default(uuid())
  email    String @unique @db.VarChar(255)
  password String @db.VarChar(255)
}

